<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.arafeh.base.view.activities.report.ReportsActivity">

    <fragment
        android:id="@+id/extendedMapFragment"
        android:name="com.arafeh.base.view.fragments.map.ExtendedMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <com.arafeh.base.view.layouts.ViewDialog
        android:id="@+id/dialogView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_margin="16dp" />
</RelativeLayout>
